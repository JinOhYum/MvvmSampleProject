<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 공통 팝업 버튼 -->
    <style name="common_popup_btn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/length_40</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_13</item>
    </style>

    <!-- 공통 팝업 취소 버튼 -->
    <style name="common_cancel" parent="common_popup_btn">
        <item name="android:background">@drawable/selector_btn_negative</item>
        <item name="android:textColor">@drawable/selector_btn_negative</item>
        <item name="android:text">@string/common_cancel</item>
    </style>

    <!-- 공통 팝업 확인 버튼 -->
    <style name="common_confirm" parent="common_popup_btn">
        <item name="android:background">@drawable/selector_btn_positive</item>
        <item name="android:textColor">@drawable/selector_btn_positive</item>
        <item name="android:text">@string/common_ok</item>
    </style>

    <!-- 공통 취소 버튼 -->
    <style name="common_negative">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/length_50</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_15</item>
        <item name="android:background">@drawable/selector_btn_negative</item>
        <item name="android:textColor">@drawable/selector_btn_negative</item>
        <item name="android:text">@string/common_cancel</item>
    </style>

    <!-- 공통 확인 버튼 -->
    <style name="common_positive">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/length_50</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_15</item>
        <item name="android:background">@drawable/selector_btn_confirm</item>
        <item name="android:textColor">@drawable/selector_btn_confirm</item>
        <item name="android:text">@string/common_ok</item>
    </style>

    <!-- 공통 하단 확인 버튼 -->
