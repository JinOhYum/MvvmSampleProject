<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <layer-list>
            <item android:drawable="@drawable/widget_2_graph_bg_bk">
                <scale android:scaleWidth="100%">
                    <shape android:shape="rectangle">
                        <corners android:radius="@dimen/length_1" />
                        <solid android:color="@color/colord9d9d9" />
                    </shape>
                </scale>
            </item>
        </layer-list>
    </item>
    <item
        android:id="@android:id/progress"
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">

        <clip>
            <layer-list>
                <item>
                    <scale android:scaleWidth="100%">
                        <shape android:shape="rectangle">
                            <corners android:radius="@dimen/length_1" />
                            <solid android:color="@color/color5ddfde" />
                        </shape>
                    </scale>
                </item>
            </layer-list>
        </clip>
    </item>
</layer-list>